(jbuild_version 1)

(executables
 ((names (example knock knock_spel fact))
  (libraries (wcs-lib wcs-api))))

(alias
 ((name   runtest)
  (deps   (example.exe))
  (action (run ${<}))))

(alias
 ((name   runtest)
  (deps   (knock.exe))
  (action (run ${<} -print))))

(alias
 ((name   runtest)
  (deps   (knock_spel.exe))
  (action (run ${<} -print))))

(alias
 ((name   runtest)
  (deps   (fact.exe))
  (action (run ${<}))))

